.alignself-flexend { -ms-flex-item-align: end; align-self: flex-end; }
.alignself-center { -ms-flex-item-align: center; -ms-grid-row-align: center; align-self: center; }
.alignself-baseline { -ms-flex-item-align: baseline; align-self: baseline; }
.alignself-stretch { -ms-flex-item-align: stretch; -ms-grid-row-align: stretch; align-self: stretch; }
.justifycontent-flexstart { -webkit-box-pack: start; -ms-flex-pack: start; justify-content: flex-start; }
.justifycontent-flexend { -webkit-box-pack: end; -ms-flex-pack: end; justify-content: flex-end; }
.justifycontent-center { -webkit-box-pack: center; -ms-flex-pack: center; justify-content: center; }
